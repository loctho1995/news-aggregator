{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@routes/*": [
        "src/routes/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@controllers/*": [
        "src/controllers/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@config/*": [
        "src/config/*"
      ],
      "@constants/*": [
        "src/constants/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "dist",
    "build"
  ]
}